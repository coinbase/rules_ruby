Gem::Specification.new do |s|
  s.name                     = "{name}"
  s.summary                  = "{name}"
  s.authors                  = {authors}
  s.version                  = "{version}"
  s.licenses                 = {licenses}
  s.files                    = {srcs}
  s.require_paths            = {require_paths}
  s.gem_runtime_dependencies = {gem_runtime_dependencies}
end
